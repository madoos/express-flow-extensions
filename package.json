{
  "name": "express-flow-extensions",
  "version": "0.0.0",
  "description": "Set of tools to accelerate the development with expres",
  "main": "index.js",
  "dependencies": {
    "bluebird": "^3.5.1",
    "celebrate": "^7.0.6",
    "http-status": "^1.0.1",
    "is-promise": "^2.1.0",
    "module-alias": "^2.0.1",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "conventional-changelog-cli": "^1.3.4",
    "coveralls": "^3.0.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "express": "^4.16.3",
    "http-server": "^0.9.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "mocha": "^5.1.0",
    "nsp": "^2.6.3",
    "nyc": "^11.0.2",
    "prettier": "^1.7.0",
    "promisify-supertest": "^1.0.0",
    "standard": "^10.0.3",
    "supertest": "^3.0.0",
    "validate-commit": "^3.2.1"
  },
  "scripts": {
    "changelog":
      "conventional-changelog -p eslint -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "commitmsg": "validate-commit-msg -p eslint",
    "lint": "eslint */**/*.js -f html -o ./test/reports/linter/index.html",
    "test": "nyc --reporter=lcov --temp-directory=coverage node test/index.js",
    "make:publish": "bin/publish.sh",
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "serve:coverage": "http-server coverage/ -o",
    "serve:linter": "http-server test/report/linter -o"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madoos/express-flow-extensions"
  },
  "author": {
    "name": "Maurice Dom√≠nguez",
    "email": "maurice.ronet.dominguez@gmail.com",
    "url": "https://github.com/madoos"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/madoos/express-flow-extensions/issues"
  },
  "homepage": "https://github.com/madoos/express-flow-extensions#readme",
  "keywords": [
    "express",
    "return",
    "router",
    "flow",
    "tools",
    "compose",
    "middleware",
    "route",
    "handler",
    "fp"
  ]
}
